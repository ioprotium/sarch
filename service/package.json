      }
    },
    "testMatch": [
      "**/*/*.spec.+(ts|js)"
    ]
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^3.0.0",
    "express": "^4.16.4",
    "firebase-admin": "^6.3.0",
    "lodash": "^4.17.11",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/config": "^0.0.34",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "jest": "^23.6.0",
    "js-yaml": "^3.12.0",
    "nodemon": "^1.18.7",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1"
  }
}
